{
    "coc.preferences.formatOnSaveFiletypes": ["java", "rust", "c", "cpp"],
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "languageserver": {
        "arduino": {
            "command": "~/Documents/ARDUINO/arduino-language-server/arduino-language-server",
            "rootPatterns": ["*.ino"],
            "filetypes":["arduino"],
            "args": ["-cli", "~/bin/arduino-cli", "-clangd", "/usr/bin/clangd", "-cli-config", 
            "~/.arduino15/arduino-cli.yaml", "-fqbn", "arduino:avr:uno"]
        }
    }
}
